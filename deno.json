{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "name": "@flowcore/sdk",
  "description": "Flowcore SDK",
  "version": "0.1.3",
  "license": "MIT",
  "exports": "./src/main.ts",
  "tasks": {},
  "imports": {
    "@sinclair/typebox": "npm:@sinclair/typebox@0.32.15",
    "@std/semver": "jsr:@std/semver@^1.0.3"
